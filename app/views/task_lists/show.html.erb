<h1><%= @task_list[:name] %></h1>

<%= flash[:notice] %>

<ul>
<%@tasks.to_a.each do |task|%>
<li><%= task["task"] %> - <%= task["date"] %></li>
    <%= link_to 'Delete', "task/#{task[:id]}/delete", :method => :delete %>
    <%= link_to 'Complete', "task/#{task[:id]}/complete", :method => :patch %>
<% end %>
</ul>


<h4><%= link_to 'Back', root_path %></h4>
